using System;
using System.Collections.Generic;

namespace ConsoleApp12
{
    class Program
    {
        static void Main(string[] args)
        {
            Dictionary<char, string> dictionary = new Dictionary<char, string>();

            dictionary['a'] = "All";
            dictionary['b'] = "Ball";
            dictionary['c'] = "Call";

            dictionary.Remove('c');
        }
    }
}
